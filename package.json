{
  "name": "@outsidecube/db-api-sync",
  "version": "0.3.0",
  "description": "Local/remote synchronization library targeted to offline first apps",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:outsidecube/db-api-sync.git"
  },
  "keywords": [
    "boilerplate",
    "typescript"
  ],
  "author": "Juan Ignacio Garz√≥n",
  "license": "BSD 2.0",
  "bugs": {
    "url": "https://github.com/outsidecube/db-api-sync/issues"
  },
  "homepage": "https://github.com/outsidecube/db-api-sync#readme",
  "devDependencies": {
    "@types/jest": "29.5.1",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.44.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.7",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.{jsx,tsx,js}": "eslint --cache --fix"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "axios": "^1.3.6",
    "jsonpath": "^1.1.1"
  }
}
